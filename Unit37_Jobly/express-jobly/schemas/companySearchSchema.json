{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/companySearch.schema.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "name",
        "minEmployees",
        "maxEmployees"
    ],
    "properties": {
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "net"
            ]
        },
        "minEmployees": {
            "type": "integer",
            "default": 0,
            "title": "The minEmployees Schema",
            "examples": [
                10
            ]
        },
        "maxEmployees": {
            "type": "integer",
            "default": 0,
            "title": "The maxEmployees Schema",
            "examples": [
                25
            ]
        }
    },
    "examples": [{
        "name": "net",
        "minEmployees": 10,
        "maxEmployees": 25
    }]
}